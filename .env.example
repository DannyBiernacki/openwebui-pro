# App
NODE_ENV=development
PORT=3000
HOST=localhost

# Database
DATABASE_URL="postgresql://postgres:postgres@localhost:5432/openwebui_pro"
DATABASE_POOL_MIN=2
DATABASE_POOL_MAX=10

# AI
OLLAMA_HOST=http://localhost:11434
OLLAMA_MODEL=llama2:70b
OPENAI_API_KEY=your_api_key_here
OPENAI_MODEL=gpt-4

# Auth
AUTH_SECRET=your_auth_secret_here
AUTH_COOKIE_NAME=openwebui_session
AUTH_COOKIE_SECURE=true
AUTH_COOKIE_SAME_SITE=lax

# Rate Limiting
RATE_LIMIT_MAX=100
RATE_LIMIT_TIME_WINDOW=60000

# Logging
LOG_LEVEL=info
LOG_FORMAT=json

# M4 Pro Optimizations
NODE_OPTIONS="--max-old-space-size=12288 --max-semi-space-size=64"
UV_THREADPOOL_SIZE=14
